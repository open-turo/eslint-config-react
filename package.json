{
  "author": "Turo engineering",
  "description": "Turo eslint configuration for react",
  "type": "module",
  "dependencies": {
    "@open-turo/eslint-config-typescript": "18.1.20",
    "eslint-plugin-jsx-a11y": "6.10.2",
